    ;include lib/ReadKey.mips
    ;include lib/EXPEVA.mips
    INIT
    CALL Init_EXPEVA
    PUSH R0
main:
    PUSH R0
    CALL Top_EXPEVA
    ANDI R1, R1, 0x00
    POP  R0
    SW   R0, R1, 0x00
    ANDI R1, R1, 0x00
    POP  R0
    SW   R0, R1, 0x01
    ANDI R1, R1, 0x00
addi r3,r1,0xaa
    SW   R3, R1, 0x00
    CALL ReadKey
    ANDI R1, R1, 0x00
addi r3,r1,0xbb
    SW   R3, R1, 0x00
halt
