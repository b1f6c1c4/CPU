div:
    ; R1 / R0 = R1 rem R0
    ;
    ; R2 is shifts
    ; MEM[0x83] is dq
    ; {MEM[0x81],MEM[0x80]} <<
    ; ---------------------
    ; {MEM[0x82],   0x00  }
    ANDI R2, R2, 0x00   ;
    SW   R0, R2, 0x82   ; dr = R0
    SW   R1, R2, 0x80   ; dp_L = R1
    SW   R2, R2, 0x81   ; dp_H = 0
    SW   R2, R2, 0x83   ; dq = 0
    ADDI R2, R2, 0x08   ; R2 = 8
    ANDI R1, R1, 0x00   ;
loop:
    BEQ  R2, R1, done   ; if (!(R2)) goto done
    ; if (dp_H < dr) goto next
    LW   R0, R1, 0x81   ; R0 = dp_H
    LW   R2, R1, 0x82   ; R2 = dr
    SLT  R3, R0, R2     ; R3 = (R0 < R2)
    BNE  R3, R1, next   ; if (!(R3)) goto next
    ; dp -= dr
    SUB  R0, R0, R2     ; R0 -= R2
    SW   R0, R1, 0x81   ; dp_H = R0
    ; dq |= 0x01
    LW   R0, R1, 0x83   ; R0 = dq
    ORI  R0, R0, 0x01   ; R0 |= 0x01
    SW   R0, R1, 0x83   ; dq = R0
next:
    ; dp <<= 1
    LW   R0, R1, 0x80   ; R0 = dp_L
    ADD  R0, R0, R0     ; R0 <<= 1
    ADDC R3, R1, R1     ; R3 = c
    SW   R0, R1, 0x80   ; dp_L = R0
    LW   R0, R1, 0x81   ; R0 = dp_H
    ADD  R0, R0, R0     ; R0 <<= 1
    ADD  R0, R0, R3     ; R0 += R3
    SW   R0, R1, 0x81   ; dp_H = R0
    ; dq <<= 1
    LW   R0, R1, 0x83   ; R0 = dq
    ADD  R0, R0, R0     ; R0 <<= 1
    SW   R0, R1, 0x83   ; dq = R0
    ADDI R2, R2, 0xff   ; R2--
done:
    LW   R0, R1, 0x81   ; R0 = dp_H
    LW   R1, R1, 0x83   ; R1 = dq
